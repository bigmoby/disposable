{
   "name":"Router Access",
   "version":"0.1.9",
   "slug":"router_access",
   "description":"A simple tool to access a router webpage (http) inside your LAN",
   "url":"https://github.com/bigmoby/disposable",
   "arch":[
      "aarch64",
      "amd64",
      "armhf",
      "armv7",
      "i386"
   ],
   "ingress": true,
   "ingress_port": 80,
   "apparmor": false,
   "host_network": true,
   "init":false,
   "full_access": false,
   "ports": {
      "80/tcp": null
    },
    "ports_description": {
      "80/tcp": "Web interface (Not required for Ingress)"
    },
   "hassio_api":true,
   "privileged":[
     "NET_ADMIN"
   ],
   "map":[
      "ssl:rw"
   ],
   "options": {
      "ssl": true,
      "certfile": "fullchain.pem",
      "keyfile": "privkey.pem"
    },
    "schema": {
      "log_level": "list(trace|debug|info|notice|warning|error|fatal)?",
      "ssl": "bool",
      "certfile": "str",
      "keyfile": "str"
    }
 }
